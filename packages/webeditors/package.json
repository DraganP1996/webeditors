{
  "name": "webeditors-vanilla",
  "version": "0.0.69",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.cjs.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/webeditors/webeditors.esm.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/webeditors/webeditors.cjs.js"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DraganP1996/webeditors.git"
  },
  "files": [
    "dist/esm/",
    "dist/components/",
    "dist/types/",
    "loader/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "watch": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'npm run build'",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/commands": "^6.8.0",
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.10.8",
    "@codemirror/lint": "^6.8.4",
    "@codemirror/search": "^6.5.10",
    "@typescript-eslint/parser": "^8.26.1",
    "codemirror": "^6.0.1",
    "eslint": "^9.22.0",
    "eslint-linter-browserify": "^9.22.0",
    "eslint-plugin-react": "^7.37.4",
    "fast-xml-parser": "^5.0.8",
    "thememirror": "^2.0.1",
    "typescript-eslint": "^8.26.1",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "21.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@stencil/core": "^4.28.2",
    "@stencil/react-output-target": "^0.8.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0"
  },
  "license": "MIT"
}
